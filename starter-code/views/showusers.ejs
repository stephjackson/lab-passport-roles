<ul>
  <% users.forEach((user) => { %>
    <li>
      <%= user.name %> -
        <a href="/<%= user._id %>">User Profile</a>
        <a href="/<%= user._id %>/edituser">Edit Profile</a>
        <form action="/<%= user._id %>/deleteuser" method="POST">
          <button type="submit">Delete Product</button>
        </form>
    </li>
    <%  }) %>
</ul>